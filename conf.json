{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "systemName": "BImA-Notifications JS-SDK"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "private": true
  }
}
